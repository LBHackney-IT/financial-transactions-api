{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "XRay": {
    "DisableXRayTracing": "false",
    "UseRuntimeErrors": "true",
    "CollectSqlQueries": "false"
  },
  "AWS": {
    "Region": "eu-west-2"
  },
  "BatchProcessing": {
    "PerBatchCount": 5000
  },
  "AllowedHosts": "*",
  "LocalStack": {
    "UseLocalStack": true,
    "Session": {
      "AwsAccessKeyId": "my-AwsAccessKeyId",
      "AwsAccessKey": "my-AwsAccessKey",
      "AwsSessionToken": "my-AwsSessionToken",
      "RegionName": "eu-west-2"
    },
    "Config": {
      "LocalStackHost": "localhost",
      "UseSsl": false,
      "UseLegacyPorts": false,
      "EdgePort": 4566
    }
  }
}
